 {country.languages.map(language => <li key={language.iso639_1}>{language.name}</li>)}\n        </ul>\n        <img src={country.flag} height=\"100\" width=\"150\"/>\n      </div>\n    )\n  } else {\n    return ''\n  }\n}\n\nconst App = () => {\n  const [ newSearch, setNewSearch ] = useState('')\n  const [ countries, setCountries ] = useState([])\n  \n  useEffect(() => {\n    axios\n      .get('https://restcountries.eu/rest/v2/all')\n      .then(response => \n        setCountries(response.data)\n      )\n  }, [])\n\n  const filteredCountries = countries.filter(country => country.name.toLowerCase().includes(newSearch.toLowerCase()))\n    \n  const handleSearchChange = (event) => {\n    setNewSearch(event.target.value)\n  }\n\n  return (\n    <div>\n      <Filter newSearch={newSearch} handleSearchChange={handleSearchChange} />\n      <Countries countries={filteredCountries} />\n    </div>\n  )\n}\n\nexport default App;","/home/todd/sites/react-projects/full-stack-open-2020/part2/countries/src/reportWebVitals.js",[],{"ruleId":"20","replacedBy":"21"},{"ruleId":"22","replacedBy":"23"},{"ruleId":"24","severity":1,"message":"25","line":1,"column":8,"nodeType":"26","messageId":"27","endLine":1,"endColumn":13},{"ruleId":"28","severity":1,"message":"29","line":37,"column":9,"nodeType":"30","endLine":37,"endColumn":59},"no-native-reassign",["31"],"no-negated-in-lhs",["32"],"no-unused-vars","'react' is defined but never used.","Identifier","unusedVar","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-global-assign","no-unsafe-negation"]